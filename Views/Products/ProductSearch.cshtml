@model IEnumerable<ComputerStoreClassLib.Model.Product>
@{ 
    Layout = null;
}


@foreach (var item in Model) {
    <tr>
        <td>
            @item.Cipher
        </td>
        <td>
            @item.Name
        </td>
        <td>
            @item.SupplerCompany.Name
        </td>
        <td>
            @item.Category.Name
        </td>
        <td>
            @item.OperationSystem.Name
        </td>
        <td>
            @item.Price
        </td>
        <td>
            @item.CountAll
        </td>
        <td>
            @item.AmmountProducts
        </td>
        <td>
            @item.Rating
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}


